{
	"rvm state": {
		"prefix": [
			"rvm."
		],
		"body": [
			"rvm.${1|gemset_present,installed|}:"
		],
		"description": "rvm state"
	},
	"rvm.gemset_present": {
		"prefix": "rvm.gemset_present:",
		"body": [
			"rvm.gemset_present:",
			"$0"
		],
		"description": "rvm.gemset_present:"
	},
	"rvm.installed": {
		"prefix": "rvm.installed:",
		"body": [
			"rvm.installed:",
			"$0"
		],
		"description": "rvm.installed:"
	}
}