{
	"solrcloud state": {
		"prefix": [
			"solrcloud."
		],
		"body": [
			"solrcloud.${1|alias,collection|}:"
		],
		"description": "solrcloud state"
	},
	"solrcloud.alias": {
		"prefix": "solrcloud.alias:",
		"body": [
			"solrcloud.alias:",
			"$0"
		],
		"description": "solrcloud.alias:"
	},
	"solrcloud.collection": {
		"prefix": "solrcloud.collection:",
		"body": [
			"solrcloud.collection:",
			"$0"
		],
		"description": "solrcloud.collection:"
	}
}