{
	"composer state": {
		"prefix": [
			"composer."
		],
		"body": [
			"composer.${1|installed,update|}:"
		],
		"description": "composer state"
	},
	"composer.installed": {
		"prefix": "composer.installed:",
		"body": [
			"composer.installed:",
			"$0"
		],
		"description": "composer.installed:"
	},
	"composer.update": {
		"prefix": "composer.update:",
		"body": [
			"composer.update:",
			"$0"
		],
		"description": "composer.update:"
	}
}