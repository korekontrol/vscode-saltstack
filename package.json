	"engines": {
		"vscode": "^1.19.0"
	},
	"contributes": {
		"languages": [
			{
				"id": "sls",
				"aliases": [
					"SaltStack",
					"salt",
					"pillar"
				],
				"extensions": [
					".sls"
				],
				"configuration": "./jinja.configuration.json"
			},
			{
				"id": "tst",
				"aliases": [
					"SaltStack",
					"salt",
					"saltcheck"
				],
				"extensions": [
					".tst"
				],
				"configuration": "./jinja.configuration.json"
			},
			{
				"id": "jinja",
				"aliases": [
					"j2",
					"jinja2"
				],
				"extensions": [
					".jinja",
					".j2"
				],
				"configuration": "./jinja.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "jinja",
				"scopeName": "source.jinja",
				"path": "./syntaxes/jinja.json"
			},
			{
				"language": "sls",
				"scopeName": "text.yaml.jinja",
				"path": "./syntaxes/sls.json"
			}
		],
